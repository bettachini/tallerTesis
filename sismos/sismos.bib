@inreference{noauthor_richter_2024,
	title = {Richter scale},
	rights = {Creative Commons Attribution-{ShareAlike} License},
	url = {https://en.wikipedia.org/w/index.php?title=Richter_scale&oldid=1225410034},
	abstract = {The Richter scale (), also called the Richter magnitude scale, Richter's magnitude scale, and the Gutenberg–Richter scale, is a measure of the strength of earthquakes, developed by Charles Francis Richter in collaboration with Beno Gutenberg, and presented in Richter's landmark 1935 paper, where he called it the "magnitude scale". This was later revised and renamed the local magnitude scale, denoted as {ML} or {ML} .
Because of various shortcomings of the original {ML}  scale, most seismological authorities now use other similar scales such as the moment magnitude scale (Mw ) to report earthquake magnitudes, but much of the news media still erroneously refers to these as "Richter" magnitudes. All magnitude scales retain the logarithmic character of the original and are scaled to have roughly comparable numeric values (typically in the middle of the scale). Due to the variance in earthquakes,  it is essential to understand the Richter scale uses common logarithms simply to make the measurements manageable (i.e., a magnitude 3 quake factors 10³ while a magnitude 5 quake factors 105 and has seismometer readings 100 times larger).},
	booktitle = {Wikipedia},
	urldate = {2024-06-15},
	date = {2024-05-24},
	langid = {english},
	note = {Page Version {ID}: 1225410034},
	file = {Snapshot:/home/vbettachini/storage/Zotero/storage/TK9XHLVZ/Richter_scale.html:text/html},
}

