@software{camilla_lingjaerde_shapr_nodate,
	location = {The Comprehensive R Archive Network},
	title = {shapr: Explaining individual machine learning predictions with Shapley values},
	url = {https://cran.r-project.org/web/packages/shapr/vignettes/understanding_shapr.html},
	abstract = {The shapr package implements an extended version of the Kernel {SHAP} method for approximating Shapley values (Lundberg and Lee (2017)), in which dependence between the features is taken into account (Aas, Jullum, and Løland (2019)). Estimation of Shapley values is of interest when attempting to explain complex machine learning models. Of existing work on interpreting individual predictions, Shapley values is regarded to be the only model-agnostic explanation method with a solid theoretical foundation (Lundberg and Lee (2017)). Kernel {SHAP} is a computationally efficient approximation to Shapley values in higher dimensions, but it assumes independent features. Aas, Jullum, and Løland (2019) extend the Kernel {SHAP} method to handle dependent features, resulting in more accurate approximations to the true Shapley values. See the paper (Aas, Jullum, and Løland (2019)) for further details.},
	author = {{Camilla Lingjærde} and {Martin Jullum} and {Nikolai Sellereite}},
	urldate = {2024-06-16},
}

